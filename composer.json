{
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Controller\\": "app/controller/",
            "App\\Models\\": "app/models/",
            "App\\Factory\\": "app/factory/",
            "App\\Factory\\Database\\": "app/factory/database/",
            "App\\SlimQuery\\": "app/query/",
            "App\\SlimQuery\\Factory\\": "app/query/factory/",
            "App\\SlimQuery\\Bulid\\": "app/query/bulid/",
            "Database\\": "db/",
            "Database\\Config\\": "db/config/",
            "Includer\\": "src/"
        }
    },
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.6",
        "illuminate/database": "~5.1"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}